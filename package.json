{
  "name": "andromia-server",
  "version": "1.0.0",
  "description": "A Node.js server built with Ts.ED for Andromia.",
  "main": "app.js",
  "scripts": {
    "postinstall": "npm run build",
    "build": "npm run transpile",
    "transpile": "tsc --project tsconfig.json",
    "transpile:w": "tsc -w",
    "start": "nodemon --watch '**/*.ts' --ignore 'node_modules/**/*' --exec ts-node app.ts"
  },
  "author": "Alexandre Parent <alexandreparent_dev@outlook.com>",
  "contributors": [
    "Guillaume Le Blanc"
  ],
  "dependencies": {
    "@tsed/common": "^4.32.0",
    "@tsed/core": "^4.32.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "dot-env": "0.0.1",
    "express": "^4.16.4",
    "method-override": "^3.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "nodemon": "^1.18.6",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6"
  }
}
