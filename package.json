{
  "name": "andromia-server",
  "version": "1.0.0",
  "description": "A Node.js server built with Ts.ED for Andromia.",
  "repository": "https://github.com/alex-dev/andromia-server",
  "main": "build/app.js",
  "scripts": {
    "build": "npm run transpile:prod",
    "mocha:e2e": "mocha --reporter spec --check-leaks --bail build-test/e2e/**/*.spec.js",
    "mocha:e2e:no-auth": "mocha --reporter spec --checkleaks --bail --exclude build-test/e2e/explorateur*.spec.js build-test/e2e/**/*.spec.js",
    "mocha:unit": "mocha --reporter spec --checkleaks --bail build-test/tests/**/*.spec.js",
    "start:dev": "nodemon --watch './src/**/*.ts' --ignore './node_modules/**/*' --exec ts-node ./src/app.ts",
    "start": "npm run transpile:prod && node ./build/app.js",
    "test": "npm run transpile:test && npm run mocha:unit && npm run mocha:e2e",
    "test:e2e": "npm run transpile:test && npm run mocha:e2e",
    "test:e2e:no-auth": "npm run transpile:test && npm run mocha:e2e:no-auth",
    "test:unit": "npm run transpile:test && npm run mocha:unit",
    "tslint": "tslint ./src/*.ts",
    "transpile:test": "tsc --project tsconfig.test.json",
    "transpile:prod": "tsc --project tsconfig.prod.json"
  },
  "author": "Alexandre Parent <alexandreparent_dev@outlook.com>",
  "contributors": [
    "Guillaume Le Blanc"
  ],
  "dependencies": {
    "@tsed/common": "^4.32.0",
    "@tsed/core": "^4.32.0",
    "@tsed/mongoose": "^4.32.0",
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "^0.0.36",
    "@types/express": "^4.16.0",
    "@types/method-override": "^0.0.31",
    "@types/moment": "^2.13.0",
    "@types/mongoose": "^5.3.1",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "express": "^4.16.4",
    "method-override": "^3.0.0",
    "moment": "^2.22.2",
    "mongoose": "^5.3.13",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6"
  },
  "devDependencies": {
    "@tsed/testing": "^4.32.0",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/supertest": "^2.0.6",
    "chai": "^4.2.0",
    "dot-env": "^0.0.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.6",
    "supertest": "^3.3.0"
  }
}
